<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hibernate.JoinCourseEntity" table="joincourse" schema="course_registertration">
        <id name="id">
            <column name="ID" sql-type="int(11)"/>
        </id>
        <property name="mssv">
            <column name="MSSV" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="id_course">
            <column name="ID_COURSE" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>