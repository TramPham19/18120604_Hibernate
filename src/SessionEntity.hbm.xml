<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hibernate.SessionEntity" table="session" schema="course_registertration">
        <id name="id">
            <column name="ID" sql-type="int(11)"/>
        </id>
        <property name="idSemester">
            <column name="IdSemester" sql-type="int(11)"/>
        </property>
        <property name="dateBegin">
            <column name="dateBegin" sql-type="date" not-null="true"/>
        </property>
        <property name="dateEnd">
            <column name="dateEnd" sql-type="date" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>