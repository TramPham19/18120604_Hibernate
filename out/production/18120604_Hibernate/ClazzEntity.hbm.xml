<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hibernate.ClazzEntity" table="class" schema="course_registertration">
        <id name="id">
            <column name="ID" sql-type="int(11)"/>
        </id>
        <property name="countStudent">
            <column name="countStudent" sql-type="int(11)"/>
        </property>
        <property name="countMen">
            <column name="countMen" sql-type="int(11)"/>
        </property>
        <property name="countWomen">
            <column name="countWomen" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>