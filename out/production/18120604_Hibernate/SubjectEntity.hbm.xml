<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hibernate.SubjectEntity" table="subject" schema="course_registertration">
        <id name="id">
            <column name="ID" sql-type="int(11)"/>
        </id>
        <property name="subjectId">
            <column name="subjectID" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="subjectName">
            <column name="subjectName" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="credits">
            <column name="credits" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>