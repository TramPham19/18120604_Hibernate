<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hibernate.CourseEntity" table="course" schema="course_registertration">
        <id name="id">
            <column name="ID" sql-type="int(11)"/>
        </id>
        <property name="idSemester">
            <column name="IdSemester" sql-type="int(11)"/>
        </property>
        <property name="idSubject">
            <column name="IdSubject" sql-type="int(11)"/>
        </property>
        <property name="teacherName">
            <column name="teacherName" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="roomName">
            <column name="roomName" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="dayOfWeek">
            <column name="dayOfWeek" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="timeOfDay">
            <column name="timeOfDay" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="slotMax">
            <column name="slotMax" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>