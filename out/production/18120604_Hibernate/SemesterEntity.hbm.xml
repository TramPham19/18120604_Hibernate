<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hibernate.SemesterEntity" table="semester" schema="course_registertration">
        <id name="id">
            <column name="ID" sql-type="int(11)"/>
        </id>
        <property name="semesterName">
            <column name="semesterName" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="year">
            <column name="year" sql-type="int(11)"/>
        </property>
        <property name="dateBegin">
            <column name="dateBegin" sql-type="date" not-null="true"/>
        </property>
        <property name="dateEnd">
            <column name="dateEnd" sql-type="date" not-null="true"/>
        </property>
        <property name="type">
            <column name="type" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>